@import url("https://fonts.googleapis.com/css?family=Barlow:400,400i,700,900");
@import url('https://fonts.googleapis.com/css?family=Lato:400,700,900');

// COLORS
litegray = #fbfbfb
superlite = #F4FBFB
litehoney = #FFCD2B
honey = #EEBA0B
veryliteblu = #69889E
liteblu = #2C3942
mediumblu = #1E272D
darkblu = #282C2E



ffam = 'Lato', sans-serif

body
	background-color litegray

a
	text-decoration none

li
	display inline-block



.wrapper
	left 0
	top 0



.header-nav
	display flex
	align-items flex-end
	justify-content space-between
	height 48px
	margin 60px 120px 0px 120px
	//background-color green

.logo
	display flex
	align-items left

.rsbear
	height 42px


.nav-wrapper
	display flex

.main-nav
	display flex
	justify-content space-around
	margin-top 2px
	margin-bottom 6px
	width 100%
	padding-left 0
	//background-color purple

.links
	display flex
	padding 0px 60px 0px 60px
	font-family ffam
	font-size 1.2em
	font-weight 700
	color gray

.links:hover:before
	clip rect(0,190px, 190px, 0)

.links:before
	position absolute
	left 0
	top 0
	content: attr(data-content)
	display inline-block
	color honey
	width 100%
	clip rect(0, 0, 190px 0)
	transition clip cubic-bezier(0.25, 0.46, 0.45, 0.94) 500ms





/* CUSTOM SCROLLBAR */
::-webkit-scrollbar{
    width: 5px;
}

/* Track */
::-webkit-scrollbar-track {
    background-color litegray;
}

/* Handle */
::-webkit-scrollbar-thumb {
    background-color liteblu;
}
::-webkit-scrollbar-thumb:window-inactive {
  background mediumblu;
}
